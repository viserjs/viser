{
  "name": "viser-graph-vue",
  "version": "0.1.8",
  "description": "viser-graph-vue is vue of viser.",
  "repository": "viserjs/viser-graph-vue",
  "license": "MIT",
  "authors": "DT-FE",
  "files": [
    "umd",
    "es",
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "src/index.tsx",
  "scripts": {
    "build": "npm run build:ts && npm run build:cjs && npm run build:umd",
    "build:ts": "rimraf es && tsc",
    "build:cjs": "rimraf lib && NODE_ENV=commonjs BABEL_ENV=cjs babel ./es -d lib",
    "build:umd": "rimraf umd && BABEL_ENV=umd webpack --config webpack.config.js",
    "clean": "git clean -fdX .",
    "lint": "tslint src/**/*",
    "analyse": "NODE_ENV=analyse webpack --progress --config webpack.config.js"
  },
  "peerDependencies": {
    "vue": ">=1"
  },
  "dependencies": {
    "@types/node": "*",
    "viser-graph": "*",
    "vue": "^2.5.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "colors": "^1.1.2",
    "gzip-size": "^4.0.0",
    "ora": "^1.3.0",
    "pretty-bytes": "^4.0.2",
    "rimraf": "^2.6.2",
    "ts-loader": "^3.1.1",
    "tslint": "^5.5.0",
    "typescript": "^2.6.1",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.11.0",
    "webpack-cli": "^2.0.14"
  },
  "keywords": [
    "g2",
    "chart",
    "react",
    "datavis"
  ]
}
