{
  "name": "viser-ng",
  "version": "2.3.2",
  "description": "viser-ng is angular of viser.",
  "repository": "viserjs/viser-ng",
  "license": "MIT",
  "authors": "DT-FE",
  "files": [
    "lib",
    "es",
    "umd"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:ts && npm run build:cjs && npm run build:umd",
    "build:ts": "rimraf es && ngc",
    "build:cjs": "rimraf lib && cross-env NODE_ENV=commonjs BABEL_ENV=cjs babel ./es -d lib",
    "build:umd": "rimraf umd && cross-env BABEL_ENV=umd webpack --config webpack.config.js",
    "clean": "git clean -fdX .",
    "lint": "tslint src",
    "analyse": "cross-env NODE_ENV=analyse webpack --progress --config webpack.config.js"
  },
  "peerDependencies": {
    "@angular/common": "^6.0.0",
    "@angular/core": "^6.0.0",
    "reflect-metadata": "^0.1.8",
    "zone.js": "^0.7.2"
  },
  "dependencies": {
    "@types/core-js": "*",
    "@types/node": "*",
    "rxjs": "^6.2.2",
    "viser": "^2.0.0"
  },
  "devDependencies": {
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/core": "^6.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "colors": "^1.1.2",
    "cross-env": "^5.2.0",
    "gzip-size": "^4.0.0",
    "ora": "^1.3.0",
    "pretty-bytes": "^4.0.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.5.0",
    "typescript": "^2.9.1",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.11.0",
    "webpack-cli": "^2.0.14",
    "zone.js": "^0.8.5"
  },
  "keywords": [
    "g2",
    "chart",
    "angular",
    "datavis"
  ]
}
